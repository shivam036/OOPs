#include<iostream>
using namespace std;
int Shivam(int arr[], int n)
{
    int f = arr[0];
    int l = arr[n - 1];
    cout << "First element: " << f << endl;
    cout << "Last element: " << l << endl;
}
  
int main()
{
    int arr[] = { 4, 5, 7, 13, 25, 65, 98 };
    int n = sizeof(arr) / sizeof(arr[0]);
    Shivam(arr, n);
    return 0;
}
